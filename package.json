{
    "name": "shakti-express-typescript-template",
    "version": "0.0.1",
    "main": "src/index.ts",
    "license": "",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "nodemon --exec ts-node src/index.ts",
        "lint": "eslint src/",
        "format": "prettier ./src --write",
        "format.checker": "prettier ./src --check",
        "spell": "cspell lint ./src/",
        "check": "pnpm lint && pnpm format.checker && pnpm spell"
    },
    "dependencies": {
        "bcrypt": "^6.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.2",
        "lorin": "^2.0.3",
        "mongoose": "^8.15.1",
        "multer": "2.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.28.0",
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.18",
        "@types/express": "^5.0.2",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/multer": "^1.4.12",
        "@typescript-eslint/eslint-plugin": "^8.33.0",
        "@typescript-eslint/parser": "^8.33.0",
        "cspell": "^9.0.2",
        "eslint": "^9.28.0",
        "globals": "^16.2.0",
        "nodemon": "^3.1.10",
        "prettier": "^3.5.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.33.0"
    },
    "packageManager": "pnpm@10.11.0"
}
