{
    "name": "shakti-express-typescript-template",
    "version": "0.0.1",
    "main": "src/index.ts",
    "license": "",
    "scripts": {
        "build": "tsc && tsx ./src/utils/build.utils.ts",
        "start": "node dist/index.js",
        "dev": "tsx --watch src/index.ts",
        "lint": "eslint src/",
        "format": "prettier ./src --write",
        "format:checker": "prettier ./src --check",
        "spell": "cspell lint ./src/",
        "check": "pnpm lint && pnpm format:checker && pnpm spell"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "lorin": "^2.0.3",
        "mongoose": "^8.15.2",
        "multer": "2.0.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.29.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/multer": "^1.4.13",
        "@typescript-eslint/eslint-plugin": "^8.34.0",
        "@typescript-eslint/parser": "^8.34.0",
        "cspell": "^9.1.1",
        "eslint": "^9.29.0",
        "globals": "^16.2.0",
        "prettier": "^3.5.3",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.34.0"
    },
    "packageManager": "pnpm@10.12.1"
}
