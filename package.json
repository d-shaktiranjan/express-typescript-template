{
    "name": "shakti-express-typescript-template",
    "version": "0.0.1",
    "main": "src/index.ts",
    "license": "",
    "scripts": {
        "build": "tsc && tsc-alias && tsx src/utils/build.utils.ts",
        "start": "node dist/index.js",
        "typecheck": "tsc --noEmit",
        "dev": "tsx watch src/run.ts --clear-screen=false",
        "lint": "eslint src/",
        "format": "prettier ./src --write",
        "format:checker": "prettier ./src --check",
        "spell": "cspell lint ./src/",
        "check": "pnpm lint && pnpm format:checker && pnpm spell",
        "test": "vitest run"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^17.2.2",
        "express": "^5.1.0",
        "lorin": "^2.0.3",
        "mongoose": "^8.18.2",
        "multer": "2.0.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.36.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/multer": "^2.0.0",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.44.1",
        "@typescript-eslint/parser": "^8.44.1",
        "cspell": "^9.2.1",
        "eslint": "^9.36.0",
        "globals": "^16.4.0",
        "prettier": "^3.6.2",
        "supertest": "^7.1.4",
        "tsc-alias": "^1.8.16",
        "tsx": "^4.20.6",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.44.1",
        "vitest": "^3.2.4"
    },
    "packageManager": "pnpm@10.12.1"
}
