{
    "name": "shakti-express-typescript-template",
    "version": "0.0.1",
    "main": "src/index.ts",
    "license": "",
    "scripts": {
        "build": "tsc && ts-node ./src/utils/build.utils.ts",
        "start": "node dist/index.js",
        "dev": "nodemon --exec ts-node src/index.ts",
        "lint": "eslint src/",
        "format": "prettier ./src --write",
        "format:checker": "prettier ./src --check",
        "spell": "cspell lint ./src/",
        "check": "pnpm lint && pnpm format:checker && pnpm spell"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^17.2.2",
        "express": "^5.1.0",
        "lorin": "^2.0.3",
        "mongoose": "^8.18.2",
        "multer": "2.0.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.36.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/multer": "^2.0.0",
        "@typescript-eslint/eslint-plugin": "^8.44.1",
        "@typescript-eslint/parser": "^8.44.1",
        "cspell": "^9.2.1",
        "eslint": "^9.36.0",
        "globals": "^16.4.0",
        "nodemon": "^3.1.10",
        "prettier": "^3.6.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.44.1"
    },
    "packageManager": "pnpm@10.12.1"
}
